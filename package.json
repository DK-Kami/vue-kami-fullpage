{
  "name": "kami-fullpage",
  "version": "1.2.1",
  "description": "Vue.js component for fullpage scrolling on the site",
  "main": "dist/kami-fullpage.umd.js",
  "module": "dist/kami-fullpage.esm.js",
  "unpkg": "dist/kami-fullpage.min.js",
  "browser": {
    "./sfc": "src/kami-fullpage.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamiMeow/kami-fullpage.git"
  },
  "keywords": [
    "Vue.js",
    "Fullpage",
    "Fullpage scroll",
    "Scrolling"
  ],
  "author": "Kami-san",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KamiMeow/kami-fullpage/issues"
  },
  "homepage": "https://github.com/KamiMeow/kami-fullpage#readme",
  "dependencies": {
    "vue": "^2.6.8"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "vue-loader": "^15.7.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  }
}
